<p><%= link_to 'New List', new_list_path, {class: 'btn'} %></p>
<h1>To Do List</h1>
<% @lists.each do |list| %>
  <h2><%= list.title %></h2>
  <h2><%= list.description %></h2>
<%= link_to 'Edit', edit_list_path(list), {class: 'btn'} %>
<%= link_to 'Delete', list_path(list), {method: :delete, class: 'btn red'}%>
<% end %>